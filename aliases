# Unix
alias ll="ls -alh"
alias v="nvim"
alias vim="nvim"

# Reload ZSH
alias z="source $HOME/.zshrc"

# Vim
alias vimdiff="vim -d"

# Misc
alias dots="cd $HOME/.dotfiles"
alias launch="cd $HOME/src/launchschool"
alias cl="cd $HOME/src/commonlit/commonlit"
alias notes="cd $HOME/Dropbox/notes"
alias pdf="evince"
